        "react-bootstrap": "^2.6.0",
        "react-dom": "^17.0.2",
        "react-editor-js": "^2.1.0",
        "react-element-to-jsx-string": "^15.0.0",
        "react-leaflet": "^3.2.5",
        "rimraf": "^5.0.0",
        "storybook": "7.0.7",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.21",
        "typedoc-plugin-mdn-links": "^2.0.0",
        "typescript": "~4.7.4",
        "vite": "^4.3.3"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{md,less,json,yml,ts,tsx}": "prettier --write"
    },
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "targets": {
        "main": {
            "optimize": true
        }
    },
    "scripts": {
        "prepare": "husky install",
        "test": "lint-staged",
        "api-extractor": "tsc --project tsconfig.build.json --outDir dist/dts  &&  api-extractor run  &&  rimraf dist/dts",
        "build": "rimraf dist/ docs/  &&  typedoc source/  &&  vite build  &&  npm run api-extractor",
        "start": "typedoc source/  &&  open-cli docs/index.html",
        "prepublishOnly": "npm test  &&  npm run build",
        "dev:storybook": "storybook dev -p 6006",
        "build:storybook": "rimraf storybook-static  &&  storybook build -c .storybook",
        "preview:storybook": "vite preview --outDir storybook-static",
        "preview": "cd preview  &&  npm start"
    },
    "peerDependencies": {
        "@editorjs/editorjs": "^2.26.1",
        "@editorjs/paragraph": "^2.9.0",
        "classnames": "^2.3.2",
        "editorjs-html": "^3.4.2",
        "leaflet": "^1.9.3",
        "mobx": "^5.15.7",
        "mobx-react": "^6.3.1",
        "prismjs": "^1.29.0",
        "react": "^17.0.2",
        "react-bootstrap": "^2.6.0",
        "react-dom": "^17.0.2",
        "react-editor-js": "^2.1.0",
        "react-element-to-jsx-string": "^15.0.0",
        "react-leaflet": "^3.2.5"
    }
}

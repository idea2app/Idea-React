        "@types/react-dom": "^17.0.18",
        "@vitejs/plugin-react": "^4.0.0",
        "classnames": "^2.3.2",
        "editorjs-html": "^3.4.2",
        "husky": "^8.0.2",
        "leaflet": "^1.9.3",
        "less": "^4.1.3",
        "lint-staged": "^13.1.0",
        "mobx": "^5.15.7",
        "mobx-react": "^6.3.1",
        "open-cli": "^7.1.0",
        "postcss-preset-env": "^8.3.2",
